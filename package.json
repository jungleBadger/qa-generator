{
  "name": "qa_generator",
  "version": "1.0.0",
  "main": "start.js",
  "scripts": {
    "start": "node start.js",
    "reload:tailwind": "npx tailwindcss -i client/user_module/src/index.css -o client/user_module/src/output.css --watch",
    "start:dev": "npm run build:client && concurrently \"npm run livereload:server\" \"npm run livereload:client\"",
    "livereload:server": "npx nodemon --config nodemon.json",
    "build:client": "vite build --config client/user_module/vite.config.mjs --mode production",
    "livereload:client": "vite serve --config client/user_module/vite.config.mjs --host",
    "lint": "npx eslint --fix --config eslint.config.mjs",
    "format": "prettier --write . --config .prettierrc.config.mjs",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "authors": [
    "jungleBadger",
    "alexiabalionis"
  ],
  "license": "MIT",
  "description": "",
  "dependencies": {
    "@fastify/cookie": "^9.3.1",
    "@fastify/formbody": "^7.4.0",
    "@fastify/passport": "^2.5.0",
    "@fastify/session": "^10.9.0",
    "@fastify/static": "^7.0.4",
    "axios": "^1.7.2",
    "dotenv": "^16.4.5",
    "fastify": "^4.28.1",
    "memorystore": "^1.6.7",
    "mongodb": "^6.8.0",
    "openai": "^4.53.2",
    "passport-auth0": "^1.4.4",
    "pino": "^9.3.2",
    "pino-pretty": "^11.2.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.25.1",
    "tiktoken": "^1.0.15",
    "uuid": "^10.0.0"
  },
  "devDependencies": {
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "@vitejs/plugin-vue": "^5.1.1",
    "autoprefixer": "^10.4.19",
    "concurrently": "^8.2.2",
    "eslint": "^9.8.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-vue": "^9.27.0",
    "globals": "^15.8.0",
    "nodemon": "^3.1.4",
    "postcss": "^8.4.40",
    "prettier": "^3.3.3",
    "sass": "^1.77.8",
    "tailwindcss": "^3.4.7",
    "vite": "^5.3.5"
  }
}
