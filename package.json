{
  "name": "qa_generator",
  "version": "1.0.0",
  "main": "start.js",
  "scripts": {
    "start": "node start.js",
    "livereload:server": "npx nodemon --config nodemon.json",
    "build:client": "vite build --config client/user_module/vite.config.mjs --mode production",
    "livereload:client": "vite serve --config client/user_module/vite.config.mjs --host",
    "start:dev": "npm run build:client && concurrently \"npm run livereload:server\" \"npm run livereload:client\"",
    "lint": "npx eslint --fix --config eslint.config.mjs",
    "format": "prettier --write . --config .prettierrc.config.mjs",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "authors": [
    "jungleBadger",
    "alexiabalionis"
  ],
  "license": "MIT",
  "description": "",
  "dependencies": {
    "@fastify/static": "^7.0.4",
    "axios": "^1.7.2",
    "dotenv": "^16.4.5",
    "fastify": "^4.28.0",
    "mongodb": "^6.8.0",
    "openai": "^4.53.2",
    "pino": "^9.2.0",
    "pino-pretty": "^11.2.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "tiktoken": "^1.0.15",
    "uuid": "^10.0.0"
  },
  "devDependencies": {
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "@vitejs/plugin-vue": "^5.0.5",
    "concurrently": "^8.2.2",
    "eslint": "^9.5.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-vue": "^9.26.0",
    "globals": "^15.5.0",
    "nodemon": "^3.1.3",
    "prettier": "^3.3.2",
    "sass": "^1.77.5",
    "vite": "^5.3.1"
  }
}
